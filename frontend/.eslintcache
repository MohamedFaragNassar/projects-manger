t </Link>\r\n                </section>\r\n                {loading?<span>loading</span>:error?<Status isOpen={true} message={error.message} />:null}\r\n            </form>\r\n            \r\n    </>\r\n}\r\n\r\nexport default Login\r\n","D:\\PROGRAMMING\\projects\\Project-mangment\\Project-Manger-App\\frontend\\src\\Constants\\projectConstants.js",[],["291","292"],"D:\\PROGRAMMING\\projects\\Project-mangment\\Project-Manger-App\\frontend\\src\\queries\\userQueries.js",[],"D:\\PROGRAMMING\\projects\\Project-mangment\\Project-Manger-App\\frontend\\src\\components\\Spinner.js",[],"D:\\PROGRAMMING\\projects\\Project-mangment\\Project-Manger-App\\frontend\\src\\components\\Status.js",["293","294"],"import React, { useEffect, useState } from 'react'\r\n\r\nconst Status = (props) => {\r\n    return <>\r\n        <div id=\"status\">\r\n            <span className={`status-${props.status}`}><i class=\"fas fa-times-circle\"></i> </span>\r\n            <span className=\"status-message\">{props.message}</span>\r\n        </div>\r\n    </>\r\n    \r\n}\r\n\r\nexport default Status;\r\n","D:\\PROGRAMMING\\projects\\Project-mangment\\Project-Manger-App\\frontend\\src\\components\\UnRegisteredRoute.js",["295"],"D:\\PROGRAMMING\\projects\\Project-mangment\\Project-Manger-App\\frontend\\src\\store.js",[],"D:\\PROGRAMMING\\projects\\Project-mangment\\Project-Manger-App\\frontend\\src\\components\\AddUsers.js",["296","297","298","299","300","301","302"],"D:\\PROGRAMMING\\projects\\Project-mangment\\Project-Manger-App\\frontend\\src\\components\\ProjectCard.js",["303","304"],"import React, { useEffect, useRef } from 'react'\r\nimport {Link} from 'react-router-dom'\r\nimport {hideOrShow} from '../helpers/helpers'\r\nimport {useMutation} from '@apollo/client'\r\nimport {deleteFavoritesMutation,deleteProjectMutation,getProjectsQuery} from '../queries/projectQueries'\r\n\r\n\r\n\r\nconst ProjectCard = (props) => {\r\n   const project = props.project\r\n   const [delFavorites] = useMutation